<div class="container-md mt-5 bg-light p-5">
    {{> backToDashboard}}
    <div class="row col-12">
    <div class="dropdown m-3">
      <a class="btn btn-danger dropdown-toggle text-light" href="#" role="button" id="dropdownMenuLink"
        data-bs-toggle="dropdown" aria-expanded="false">
        Select an Event
      </a>
      <ul class="dropdown-menu" aria-labelledby="dropdownMenuLink">
        {{#each events}}
        <li><a class="dropdown-item" href="../tables/{{adminId}}?eventId={{this.eventId}}">{{this.eventName}}</a></li>
        {{/each}}
      </ul>
    </div>
  </div>
  <div class="d-flex flex-wrap">
    {{#each guests}}
    {{> QRcode}}

    {{!-- <li>{{this.table}}</li>
    <li>{{this.guests}} </li> --}}
    {{/each}}
  </div>
  <div>
    <button type="submit" class="btn btn-danger text-light register" id="qrButton">Get QR Code</button>
  </div>
</div>